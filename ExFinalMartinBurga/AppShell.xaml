<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ExFinalMartinBurga.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ExFinalMartinBurga"
    Shell.FlyoutBehavior="Disabled"
    Title="ExFinalMartinBurga">

    <TabBar>
        <ShellContent
            Title="Agregar"
            Icon="add.png"
            ContentTemplate="{DataTemplate views:AgregarEquipoPage}" />

        <ShellContent
            Title="Equipos"
            Icon="list.png"
            ContentTemplate="{DataTemplate views:ListaEquiposPage}" />

        <ShellContent
            Title="Logs"
            Icon="file.png"
            ContentTemplate="{DataTemplate views:LogRegistroPage}" />
    </TabBar>

</Shell>
