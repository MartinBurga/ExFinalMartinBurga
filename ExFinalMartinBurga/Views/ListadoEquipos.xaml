<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ExFinalMartinBurga.Views.ListadoEquipos"
             Title="ListadoEquipos">
    <VerticalStackLayout>
        <CollectionView ItemsSource="{Binding ListadoEquipos}">
            <CollectionView.ItemTemplate>
                <DataTemplate>
                    <Label Text="{Binding Dispositivo}"/>
                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>
                        
    </VerticalStackLayout>
</ContentPage>